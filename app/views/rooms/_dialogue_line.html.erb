<div class="dialogue_line<%= "_player" if dialogue_line.line_generator.name == 'player_response'%>" id="dialogue_line_<%= dialogue_line.id.to_s + "_" + d_time_stamp %>">
  <%= image_tag(avatar_thumbnail) %>
  <h4><%= h(avatar.short_name) %> says:</h4>
  <p>
    <%= wrap_dialogue(avatar, dialogue_line.content) %>
  </p>
</div>
